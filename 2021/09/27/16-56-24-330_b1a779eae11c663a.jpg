peg","source_url":"https:\/\/techcrunch.com\/wp-content\/uploads\/2021\/05\/tesla-gettyimages.jpg?w=680"},"1536x1536":{"file":"tesla-gettyimages.jpg?resize=1536,1024","width":1536,"height":1024,"mime_type":"image\/jpeg","source_url":"https:\/\/techcrunch.com\/wp-content\/uploads\/2021\/05\/tesla-gettyimages.jpg?w=1536"},"2048x2048":{"file":"tesla-gettyimages.jpg?resize=2048,1365","width":2048,"height":1365,"mime_type":"image\/jpeg","source_url":"https:\/\/techcrunch.com\/wp-content\/uploads\/2021\/05\/tesla-gettyimages.jpg?w=2048"},"guest-author-32":{"file":"tesla-gettyimages.jpg?resize=32,32","width":32,"height":32,"mime_type":"image\/jpeg","source_url":"https:\/\/techcrunch.com\/wp-content\/uploads\/2021\/05\/tesla-gettyimages.jpg?w=32&h=32&crop=1"},"guest-author-50":{"file":"tesla-gettyimages.jpg?resize=50,50","width":50,"height":50,"mime_type":"image\/jpeg","source_url":"https:\/\/techcrunch.com\/wp-content\/uploads\/2021\/05\/tesla-gettyimages.jpg?w=50&h=50&crop=1"},"guest-author-64":{"file":"tesla-gettyimages.jpg?resize=64,64","width":64,"height":64,"mime_type":"image\/jpeg","source_url":"https:\/\/techcrunch.com\/wp-content\/uploads\/2021\/05\/tesla-gettyimages.jpg?w=64&h=64&crop=1"},"guest-author-96":{"file":"tesla-gettyimages.jpg?resize=96,96","width":96,"height":96,"mime_type":"image\/jpeg","source_url":"https:\/\/techcrunch.com\/wp-content\/uploads\/2021\/05\/tesla-gettyimages.jpg?w=96&h=96&crop=1"},"guest-author-128":{"file":"tesla-gettyimages.jpg?resize=128,128","width":128,"height":128,"mime_type":"image\/jpeg","source_url":"https:\/\/techcrunch.com\/wp-content\/uploads\/2021\/05\/tesla-gettyimages.jpg?w=128&h=128&crop=1"},"concierge-thumb":{"file":"tesla-gettyimages.jpg?resize=50,33","width":50,"height":33,"mime_type":"image\/jpeg","source_url":"https:\/\/techcrunch.com\/wp-content\/uploads\/2021\/05\/tesla-gettyimages.jpg?w=50"},"full":{"file":"tesla-gettyimages.jpg","width":1024,"height":683,"mime_type":"image\/jpeg","source_url":"https:\/\/techcrun